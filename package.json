{
  "name": "buitar-editor",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/buitar-editor.umd.cjs",
  "module": "./dist/buitar-editor.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/buitar-editor.js",
    "require": "./dist/buitar-editor.umd.cjs"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build:ghpage": "BASE_URL=/buitar-editor/ vite build",
    "build:lib": "vite build --config vite.config.lib.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@buitar/svg-chord": "^0.0.8",
    "@buitar/to-guitar": "^0.0.14",
    "@coderline/alphatab": "^1.3.0-alpha.1015",
    "abcjs": "^6.3.0",
    "classnames": "^2.5.1",
    "image-extensions": "^1.1.0",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slate": "^0.102.0",
    "slate-history": "^0.100.0",
    "slate-react": "^0.102.0"
  },
  "devDependencies": {
    "@types/is-hotkey": "^0.1.10",
    "@types/is-url": "^1.2.32",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.38",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  }
}
